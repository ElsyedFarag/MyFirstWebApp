@{
    ViewBag.BlankTitle = "Order Management";
    ViewBag.CardTitle = "List Of Orders";
}


<table id="mytable" style="width:100%" class="table table-hover">
    <thead>
        <tr>
            <th style="width: 5%">
                Id
            </th>
            <th style="width: 15%">
                Name
            </th>
            <th style="width: 20%">
                Phone Number
            </th>
            <th style="width: 15%">
                Email
            </th>
            <th style="width: 10%">
                OrderStatus
            </th>
            <th style="width: 10%">
                Total Price
            </th>
            <th style="width:15%">
                Shipping Date
            </th>
            <th style="width:30%">
            </th>
        </tr>
    </thead>
</table>

@section Scripts {
    <script src="~/js/order.js"></script>
}